#title=album and artist  test_DELME
#head=title:h
#vcollapse=head
#vmin=pic:y+pic:h+2
#left=pic:w+2
#title2:	text(markup='<b><big>'.pesc($artist).'</big></b>',pad=2,x=-title2:w)
##title2:	text(markup='by <b><big>'.pesc($artist).'</big></b>',pad=2,x=title:w)
